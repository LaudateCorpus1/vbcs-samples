{

  "title": "",
  "description": "",
  "variables": {
    "employeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "page:getallEmployeeResponse"
      }
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallEmployeeResponse": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "@decorators.getallEmployeeResponse2": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "getallEmployeeResponse": {
      "items": [
        {
          "firstName": "string",
          "id": "number",
          "lastName": "string",
          "phoneNumber": "string",
          "salary": "number"
        }
      ]
    },
    "getallEmployeeResponse2": {
      "items": [
        {
          "firstName": "string",
          "id": "number",
          "lastName": "string",
          "phoneNumber": "string",
          "salary": "number"
        }
      ]
    }
  },
  "chains": {
    "performSwipeOperationChain": {
      "root": "switch",
      "variables": {
        "navigationItem": {
          "type": "string",
          "required": true,
          "input": "fromCaller"
        },
        "employeeId": {
          "type": "string",
          "required": true,
          "input": "fromCaller"
        },
        "rowData": {
          "type": "any",
          "required": false,
          "input": "fromCaller"
        }
      },
      "actions": {
        "switch": {
          "module": "vb/action/builtin/switchAction",
          "parameters": {
            "caseValue": "[[$chain.variables.navigationItem]]",
            "possibleValues": [
              "rightedit"
            ]
          },
          "outcomes": {
            "rightedit": "navigateToEditPage"
          }
        },
        "navigateToEditPage": {
          "module": "vb/action/builtin/navigateToPageAction",
          "parameters": {
            "page": "main-editpage",
            "history": "push",
            "params": {
              "employee_ID": "{{ $variables.employeeId }}"
            }
          }
        }
      }
    }
  },
  "eventListeners": {
    "SwipeActionEventListener": {
      "chains": [
        {
          "chainId": "performSwipeOperationChain",
          "parameters": {
            "navigationItem": "[[$event.target.value]]",
            "employeeId": "[[$current.key]]",
            "rowData": "[[$current.data]]"
          }
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-ext-mobile-page-template": {
        "path": "vb/components/mobile-page-template/loader"
      },
      "oj-list-view": {
        "path": "ojs/ojlistview"
      },
      "oj-vb-list-item": {
        "path": "vb/components/list-item/loader"
      },
      "oj-label": {
        "path": "ojs/ojlabel"
      },
      "oj-swipe-actions": {
        "path": "ojs/ojswipeactions"
      },
      "oj-option": {
        "path": "ojs/ojoption"
      }
    }
  }
}